{
    "name": "supa/supa",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
     "version": "1.1.0",
    "autoload": {
        "psr-4": {
            "SUPA\\": "core/"
        },
        "files": [
            "core/_index.php"
        ]
    },"scripts": {
        "post-install-cmd": [
            "@php -r \"copy('vendor/devdavehq/supa/index.php', 'index.php');\""
        ]
    },
    "authors": [
        {
            "name": "David"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/devdavehq/supa"
        }
    ],
    "require": {
        "mongodb/mongodb": "^1.0",
        "phpmailer/phpmailer": "^6.9",
        "cboden/ratchet": "^0.4.4",
        "react/event-loop": "^1.0",
        "react/http": "^1.0",
        "react/socket": "^1.0",
        "firebase/php-jwt": "^6.10",
        "php": ">=7.4"
    }
}


